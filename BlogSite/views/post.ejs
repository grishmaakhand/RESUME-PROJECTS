<!DOCTYPE html>
<html lang="en">
  <!-- head-->
  <%-include("Layout/herder");-%>

  <body>
    <!-- Navigation-->
    <%-include("Layout/navbar");-%>
    <!-- Page Header-->
    <header
      class="masthead"
      style="background-image: url('assets<%= passpost.image%>')"
    >
      <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
          <div class="col-md-10 col-lg-8 col-xl-7">
            <div class="post-heading">
              <h1><%= passpost.title%></h1>
              <h2 class="subheading"><%= passpost.description%></h2>
              <span class="meta">
                Posted by
                <a href="#!"><%= passpost.userid.name%></a>
                on <%= passpost.datepost%>
              </span>
            </div>
          </div>
        </div>
      </div>
    </header>

    <!-- Footer-->
    <%-include("Layout/footer");-%>
    <!-- Bootstrap core JS-->
    <%-include("Layout/script");-%>
  </body>
</html>
